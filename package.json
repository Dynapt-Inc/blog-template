{
  "name": "@caleblawson/blog-shell",
  "version": "0.1.3",
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf dist",
    "build:ts": "echo 'Compiling TypeScript...' && tsc -p tsconfig.build.json && echo 'Rewriting path aliases...' && tsc-alias -p tsconfig.build.json && echo 'TypeScript build complete!'",
    "build": "npm run clean && npm run build:ts && node ./scripts/copy-assets.cjs",
    "prepublishOnly": "npm run build",
    "start": "next start",
    "lint": "eslint",
    "setup:secrets": "node --env-file=.env.local ./scripts/secrets-setup.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./dist/styles/globals.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles/globals.css": {
      "default": "./dist/styles/globals.css"
    }
  },
  "dependencies": {
    "mysql2": "^3.11.3",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "libsodium-wrappers": "^0.7.13",
    "rimraf": "^5.0.0",
    "tailwindcss": "^4",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "typescript": "^5"
  }
}
